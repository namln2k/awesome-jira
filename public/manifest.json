{
  "manifest_version": 3,
  "short_name": "Awesome Jira",
  "name": "Awesome Jira by NamLN",
  "version": "1.0",
  "description": "A Chrome extension by NamLN that customizes Jira interface",
  "action": {
    "default_popup": "index.html",
    "default_title": "Awesome Jira",
    "default_icon": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
